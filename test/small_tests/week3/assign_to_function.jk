/*
In this program I use functions on the left and right hand side of assignments
Value: 10
*/

func1() {
  int a = 0;
  *a;
}

func2() {
  int b = 0;
  *b;
}

func3() {
  6;
}

func4() {
  int x = 4;
  int y = 8;
  if (1 == 1) {
    *x;
  } else {
    *y;
  }
}

main() {
  /* this block can also be written as: func2() = func3() + func4(); */
  int a = func1() + func3();
  int b = *a + func2() + func4();
  *b;
}
